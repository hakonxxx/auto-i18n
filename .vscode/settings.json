{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "cSpell.words": [
    "Cardano",
    "Chakra",
    "chakra's",
    "Checkmark",
    "commonRange",
    "CRYPTOCOM",
    "Defi",
    "dogecoin",
    "Etags",
